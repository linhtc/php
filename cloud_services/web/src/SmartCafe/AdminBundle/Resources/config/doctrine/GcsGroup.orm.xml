<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\GcsGroup" table="gcs_group">
    <indexes>
      <index name="IDX_BD59CD72E5C4B06" columns="usercreated"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_BD59CD75E237E06" columns="name"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="roles" type="text" column="roles" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="datecreated" type="datetime" column="datecreated" nullable="true"/>
    <field name="delif" type="boolean" column="delif" nullable="false"/>
    <many-to-one field="usercreated" target-entity="GcsUser">
      <join-columns>
        <join-column name="usercreated" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="role" target-entity="GcsRole" inversed-by="group">
      <join-table name="group_role">
        <join-columns>
          <join-column name="group_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="role_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>

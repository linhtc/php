<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\GcsContact" table="gcs_contact">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fullname" type="string" column="fullname" length="255" nullable="true"/>
    <field name="companyname" type="string" column="companyname" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true"/>
    <field name="fax" type="string" column="fax" length="255" nullable="true"/>
    <field name="cellphone" type="string" column="cellphone" length="255" nullable="true"/>
    <field name="address1" type="string" column="address1" length="255" nullable="true"/>
    <field name="address2" type="string" column="address2" length="255" nullable="true"/>
    <field name="subject" type="integer" column="subject" nullable="true"/>
    <field name="content" type="text" column="content" nullable="true"/>
    <field name="sendmail" type="integer" column="sendmail" nullable="true"/>
    <field name="read" type="integer" column="read" nullable="true"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
  </entity>
</doctrine-mapping>

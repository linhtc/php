<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\GcsLithium" table="gcs_lithium">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="voltage" type="integer" column="voltage" nullable="true"/>
    <field name="current" type="integer" column="current" nullable="true"/>
    <field name="stateCharge" type="integer" column="state_charge" nullable="true"/>
    <field name="stateHealth" type="integer" column="state_health" nullable="true"/>
    <field name="impedance" type="integer" column="impedance" nullable="true"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
    <field name="station" type="string" column="station" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>

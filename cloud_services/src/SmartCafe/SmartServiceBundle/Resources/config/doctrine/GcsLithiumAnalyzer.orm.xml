<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\GcsLithiumAnalyzer" table="gcs_lithium_analyzer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="modelName" type="string" column="model_name" length="255" nullable="true"/>
    <field name="stateOfCharge" type="float" column="state_of_charge" precision="10" scale="0" nullable="true"/>
    <field name="stateOfHealth" type="float" column="state_of_health" precision="10" scale="0" nullable="true"/>
    <field name="value" type="float" column="value" precision="10" scale="0" nullable="true"/>
    <field name="date" type="datetime" column="date" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\GcsResource" table="gcs_resource">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="controller" type="string" column="controller" length="500" nullable="true"/>
    <field name="route" type="string" column="route" length="500" nullable="true"/>
    <field name="name" type="string" column="name" length="500" nullable="true"/>
    <field name="parent" type="string" column="parent" length="255" nullable="true"/>
    <field name="routeName" type="string" column="route_name" length="500" nullable="true"/>
    <field name="action" type="string" column="action" length="500" nullable="true"/>
    <field name="actionName" type="string" column="action_name" length="500" nullable="true"/>
    <field name="enabled" type="boolean" column="enabled" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\SellphoneCustomer" table="sellphone_customer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="100" nullable="true"/>
    <field name="lastname" type="string" column="lastname" length="100" nullable="true"/>
    <field name="fullname" type="string" column="fullname" length="200" nullable="true"/>
    <field name="email" type="string" column="email" length="100" nullable="true"/>
    <field name="phone" type="string" column="phone" length="100" nullable="true"/>
    <field name="address" type="string" column="address" length="100" nullable="true"/>
    <field name="company" type="string" column="company" length="100" nullable="true"/>
    <field name="fax" type="string" column="fax" length="100" nullable="true"/>
    <field name="city" type="string" column="city" length="100" nullable="true"/>
    <field name="country" type="integer" column="country" nullable="true"/>
    <field name="postcode" type="string" column="postcode" length="20" nullable="true"/>
  </entity>
</doctrine-mapping>

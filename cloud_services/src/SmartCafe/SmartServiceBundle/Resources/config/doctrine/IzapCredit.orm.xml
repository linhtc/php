<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gcs\AdminBundle\Entity\IzapCredit" table="izap_credit">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="machineSn" type="string" column="machine_sn" length="255" nullable="true"/>
    <field name="credit" type="integer" column="credit" nullable="true"/>
    <field name="datetime" type="datetime" column="datetime" nullable="true"/>
    <field name="usercreate" type="string" column="usercreate" length="100" nullable="true"/>
    <field name="synced" type="integer" column="synced" nullable="true"/>
    <field name="delif" type="integer" column="delif" nullable="true"/>
  </entity>
</doctrine-mapping>
